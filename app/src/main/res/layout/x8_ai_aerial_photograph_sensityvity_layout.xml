<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_aerail_sensity"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clickable="true"
    android:visibility="gone">

    <TextView
        android:id="@+id/tv_braking_sensity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="13.0dip"
        android:text="@string/x8_fc_sensitivity_brake_sens"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39" />

    <TextView
        android:id="@+id/tv_braking_sensity_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/x8s_ai_point_to_point__excute_confirm_layout_x5"
        android:layout_toRightOf="@+id/tv_braking_sensity"
        android:text="10"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39" />

    <com.fimi.app.x8s.widget.X8PlusMinusSeekBar
        android:id="@+id/sb_braking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_braking_sensity_value"
        android:layout_marginLeft="20.0dip"
        android:layout_marginRight="20.0dip" />

    <TextView
        android:id="@+id/tv_braking_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sb_braking"
        android:layout_marginLeft="16.0dip"
        android:text="@string/x8_ai_aerail_graph_next_tip1"
        android:textColor="@color/white_90"
        android:textSize="10.0dip" />

    <TextView
        android:id="@+id/tv_yaw_sensity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_braking_tip"
        android:layout_marginLeft="13.0dip"
        android:layout_marginTop="8.0dip"
        android:text="@string/x8_fc_sensitivity_yaw_sens"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39" />

    <TextView
        android:id="@+id/tv_yaw_sensity_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_braking_tip"
        android:layout_marginLeft="@dimen/x8s_ai_point_to_point__excute_confirm_layout_x5"
        android:layout_toRightOf="@+id/tv_yaw_sensity"
        android:text="10"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39" />

    <com.fimi.app.x8s.widget.X8PlusMinusSeekBar
        android:id="@+id/sb_yaw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_yaw_sensity_value"
        android:layout_marginLeft="20.0dip"
        android:layout_marginRight="20.0dip" />

    <TextView
        android:id="@+id/tv_yaw_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sb_yaw"
        android:layout_marginLeft="16.0dip"
        android:layout_marginTop="8.0dip"
        android:text="@string/x8_ai_aerail_graph_next_tip2"
        android:textColor="@color/white_90"
        android:textSize="10.0dip" />
</RelativeLayout>