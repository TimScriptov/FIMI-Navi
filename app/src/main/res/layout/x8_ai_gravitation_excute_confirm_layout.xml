<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_ai_point_to_point_confirm_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentRight="true"
    android:clickable="true">

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/rl_ai_gravitation_excute_confirm"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        app:layout_heightPercent="11%">

        <ImageView
            android:id="@+id/img_ai_gravitation_excute_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/x8_img_return"
            android:visibility="visible" />

        <TextView
            android:id="@+id/tv_ai_gravitation_excute_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/x8_ai_fly_gravitation"
            android:textColor="@color/white_90"
            android:textSize="@dimen/textsize_45" />
    </com.fimi.kernel.percent.PercentRelativeLayout>

    <View
        android:id="@+id/view_gravitation_excute1"
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:background="@color/white_10"
        app:layout_marginTopPercent="11.11111%" />

    <RelativeLayout
        android:id="@+id/rl_gravitation_excute_setting"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view_gravitation_excute1">

        <RelativeLayout
            android:id="@+id/rl_gravitation_excute_videotape"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14.0dip">

            <TextView
                android:id="@+id/tv_gravitation_excute_videotape"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16.0dip"
                android:text="@string/x8_ai_fly_gravitation_excute_videotape_whole"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39"
                app:layout_marginLeftPercent="5.8333333%" />

            <com.fimi.app.x8s.widget.X8TabHost
                android:id="@+id/x8_ai_gravitation_excute_videotape"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/x8s_main_fc_item_x7"
                android:layout_marginLeft="5.0dip"
                android:layout_marginRight="30.0dip"
                android:layout_toRightOf="@+id/tv_gravitation_excute_videotape"
                android:padding="@dimen/x8s_main_fc_item_x1"
                app:bg="#00000000"
                app:default_index="1"
                app:layout_marginRightPercent="5.8333333%"
                app:lineColor="@color/white_25"
                app:lineStroke="@dimen/x8s_main_fc_item_x1"
                app:radiusC="@dimen/x8s_main_fc_item_x2"
                app:src="@array/x8_ai_gravitation_excute_value_videotape"
                app:tab_select_color="@color/tabradio_attr_selected_color"
                app:tab_space="@dimen/x8s_main_fc_item_x1"
                app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                app:text_select_color="@color/tabradio_attr_selected_text_color"
                app:text_sizeC="@dimen/textsize_39"
                app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
        </RelativeLayout>

        <Button
            android:id="@+id/btn_ai_gravitation_excute_advanced_setting"
            android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x12"
            android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x9"
            android:layout_below="@+id/rl_gravitation_excute_videotape"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="35.0dip"
            android:background="@drawable/x8_btn_gray_selector"
            android:text="@string/x8_ai_fly_gravitation_excute_more_setting"
            android:textColor="@color/tabradio_attr_unselected_text_color"
            android:textSize="@dimen/textsize_39"
            app:layout_marginBottomPercent="2.777777%" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/btn_ai_gravitation_excute_advanced_setting"
            android:layout_marginTop="30.0dip"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:paddingRight="18.0dip">

                <TextView
                    android:id="@+id/tv_ai_gravitation_excute_title1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="18.0dip"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/x8_ai_fly_gravitation_excute_tips1"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_gravitation_excute_title2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_gravitation_excute_title1"
                    android:layout_marginLeft="18.0dip"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/x8_ai_fly_gravitation_excute_tips2"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

    <Button
        android:id="@+id/btn_ai_gravitation_excute_ok"
        android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x12"
        android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x9"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/x8_btn_white_selector"
        android:text="@string/x8_ai_fly_follow_go"
        android:textColor="@color/black_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginBottomPercent="2.777777%" />

    <Button
        android:id="@+id/btn_ai_gravitation_excute_default"
        android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x12"
        android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x9"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="40.0dip"
        android:background="@drawable/x8_btn_white_selector"
        android:text="@string/x8_ai_fly_gravitation_excute_advanced_setting_default"
        android:textColor="@color/black_90"
        android:textSize="@dimen/textsize_39"
        android:visibility="gone"
        app:layout_marginBottomPercent="2.777777%" />

    <Button
        android:id="@+id/btn_ai_gravitation_excute_save"
        android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x12"
        android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x9"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="40.0dip"
        android:background="@drawable/x8_btn_white_selector"
        android:text="@string/x8_ai_fly_gravitation_excute_advanced_setting_save"
        android:textColor="@color/black_90"
        android:textSize="@dimen/textsize_39"
        android:visibility="gone"
        app:layout_marginBottomPercent="2.777777%" />

    <LinearLayout
        android:id="@+id/ll_gravitation_excute_advanced_setting"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view_gravitation_excute1"
        android:orientation="vertical"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="18.0dip"
            android:layout_marginTop="10.0dip"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/x8_ai_fly_gravitation_excute_advanced_setting_dec1"
                android:textColor="@color/white_90"
                android:textSize="@dimen/textsize_39" />

            <TextView
                android:id="@+id/tv_ai_gravitation_excute_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5.0dip"
                android:text="@string/x8_ai_fly_gravitation_excute_advanced_setting_level"
                android:textColor="@color/white_90"
                android:textSize="@dimen/textsize_39" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x11">

            <RelativeLayout
                android:id="@+id/rl_ai_gravitation_excute_level_minus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

                <View
                    android:id="@+id/view_ai_gravitation_excute_level_minus"
                    android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x3"
                    android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x1"
                    android:layout_centerInParent="true"
                    android:background="@drawable/x8_btn_minus_bg" />
            </RelativeLayout>

            <com.fimi.app.x8s.widget.X8SeekBarView
                android:id="@+id/sb_ai_gravitation_excute_level"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/rl_ai_gravitation_excute_level_plus"
                android:layout_toRightOf="@+id/rl_ai_gravitation_excute_level_minus" />

            <RelativeLayout
                android:id="@+id/rl_ai_gravitation_excute_level_plus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

                <View
                    android:id="@+id/view_ai_gravitation_excute_level_plus"
                    android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                    android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                    android:layout_centerInParent="true"
                    android:background="@drawable/x8_btn_plus_bg" />
            </RelativeLayout>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="18.0dip"
            android:layout_marginTop="10.0dip"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/x8_ai_fly_gravitation_excute_advanced_setting_dec2"
                android:textColor="@color/white_90"
                android:textSize="@dimen/textsize_39" />

            <TextView
                android:id="@+id/tv_ai_gravitation_excute_height"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5.0dip"
                android:text="@string/x8_ai_fly_gravitation_excute_advanced_setting_height"
                android:textColor="@color/white_90"
                android:textSize="@dimen/textsize_39" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/rl_ai_gravitation_excute_heigth_seekbar"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x11"
            android:layout_below="@+id/tv_ai_speed_lable">

            <RelativeLayout
                android:id="@+id/rl_ai_gravitation_excute_heigth_minus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

                <View
                    android:id="@+id/view_ai_gravitation_excute_heigth_minus"
                    android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x3"
                    android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x1"
                    android:layout_centerInParent="true"
                    android:background="@drawable/x8_btn_minus_bg" />
            </RelativeLayout>

            <com.fimi.app.x8s.widget.X8SeekBarView
                android:id="@+id/sb_ai_gravitation_excute_heigth"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/rl_ai_gravitation_excute_heigth_plus"
                android:layout_toRightOf="@+id/rl_ai_gravitation_excute_heigth_minus" />

            <RelativeLayout
                android:id="@+id/rl_ai_gravitation_excute_heigth_plus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

                <View
                    android:id="@+id/view_ai_gravitation_excute_heigth_plus"
                    android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                    android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                    android:layout_centerInParent="true"
                    android:background="@drawable/x8_btn_plus_bg" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_gravitation_excute_rotate"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14.0dip">

            <TextView
                android:id="@+id/tv_gravitation_excute_rotate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16.0dip"
                android:text="@string/x8_ai_fly_gravitation_excute_advanced_setting_rotate"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39"
                app:layout_marginLeftPercent="5.8333333%" />

            <com.fimi.app.x8s.widget.X8TabHost
                android:id="@+id/x8_ai_gravitation_excute_rotate"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/x8s_main_fc_item_x7"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="30.0dip"
                android:padding="@dimen/x8s_main_fc_item_x1"
                app:bg="#00000000"
                app:default_index="1"
                app:layout_marginRightPercent="5.8333333%"
                app:lineColor="@color/white_25"
                app:lineStroke="@dimen/x8s_main_fc_item_x1"
                app:radiusC="@dimen/x8s_main_fc_item_x2"
                app:src="@array/x8_ai_gravitation_excute_value_advanced_setting"
                app:tab_select_color="@color/tabradio_attr_selected_color"
                app:tab_space="@dimen/x8s_main_fc_item_x1"
                app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                app:text_select_color="@color/tabradio_attr_selected_text_color"
                app:text_sizeC="@dimen/textsize_39"
                app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
        </RelativeLayout>
    </LinearLayout>
</com.fimi.kernel.percent.PercentRelativeLayout>