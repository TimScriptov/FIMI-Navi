<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_ai_point_to_point_confirm_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentRight="true">

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/rl_ai_follow_confirm"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        app:layout_marginTopPercent="2.7777%">

        <ImageView
            android:id="@+id/img_ai_follow_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/x8_img_return" />

        <TextView
            android:id="@+id/tv_ai_follow_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/x8_ai_fly_return_home_title"
            android:textColor="@color/white_90"
            android:textSize="@dimen/textsize_39" />
    </com.fimi.kernel.percent.PercentRelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="@dimen/x8s_ai_return_layout_x1"
        android:background="@color/white_10"
        app:layout_marginTopPercent="11.11111%" />

    <ImageView
        android:id="@+id/img_ai_return_flag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        app:layout_marginTopPercent="13.666%" />

    <TextView
        android:id="@+id/tv_ai_follow_confirm_title1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img_ai_return_flag"
        android:layout_centerHorizontal="true"
        android:text="@string/x8_ai_fly_return_home_tip"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_36" />

    <TextView
        android:id="@+id/tv_limit_height"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_ai_follow_confirm_title1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10.0dip"
        android:text="@string/x8_ai_fly_return_home_tip2"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_36"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/rl_seekbar"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/x8s_value_seekbar_layout_x6"
        android:layout_below="@+id/tv_limit_height"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/rl_minus"
            android:layout_width="@dimen/x8s_value_seekbar_layout_x5"
            android:layout_height="@dimen/x8s_value_seekbar_layout_x5"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x8s_return_seekbar_layout_x24">

            <View
                android:id="@+id/view_minus"
                android:layout_width="@dimen/x8s_value_seekbar_layout_x3"
                android:layout_height="@dimen/x8s_value_seekbar_layout_x1"
                android:layout_centerInParent="true"
                android:background="@drawable/x8_btn_minus_bg" />
        </RelativeLayout>

        <SeekBar
            android:id="@+id/sb_value"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x8s_value_seekbar_layout_x4"
            android:layout_marginRight="@dimen/x8s_value_seekbar_layout_x4"
            android:layout_toLeftOf="@+id/rl_plus"
            android:layout_toRightOf="@+id/rl_minus"
            android:background="@null"
            android:focusable="true"
            android:maxHeight="@dimen/x8s_value_seekbar_layout_x2"
            android:minHeight="@dimen/x8s_value_seekbar_layout_x2"
            android:progressDrawable="@drawable/x8_img_seekbar_bg"
            android:thumb="@drawable/x8_img_seekbar_thum" />

        <RelativeLayout
            android:id="@+id/rl_plus"
            android:layout_width="@dimen/x8s_value_seekbar_layout_x5"
            android:layout_height="@dimen/x8s_value_seekbar_layout_x5"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x8s_return_seekbar_layout_x24">

            <View
                android:id="@+id/view_plus"
                android:layout_width="@dimen/x8s_value_seekbar_layout_x4"
                android:layout_height="@dimen/x8s_value_seekbar_layout_x4"
                android:layout_centerInParent="true"
                android:background="@drawable/x8_btn_plus_bg" />
        </RelativeLayout>
    </RelativeLayout>

    <Button
        android:id="@+id/btn_ai_follow_confirm_ok"
        android:layout_width="@dimen/x8s_ai_return_layout_x4"
        android:layout_height="@dimen/x8s_ai_return_layout_x3"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/x8s_ai_return_layout_x2"
        android:background="@drawable/x8_btn_white_selector"
        android:text="@string/x8_ai_fly_follow_ok"
        android:textColor="@color/black_90"
        android:textSize="@dimen/textsize_36"
        app:layout_marginTopPercent="2.777777%" />
</com.fimi.kernel.percent.PercentRelativeLayout>