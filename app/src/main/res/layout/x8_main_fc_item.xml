<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_rl_main_fc_item"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="none"
        android:overScrollMode="never"
        android:scrollbars="none"
        app:layout_marginBottomPercent="1.478743%w"
        app:layout_marginLeftPercent="1.478743%w"
        app:layout_marginRightPercent="1.478743%w"
        app:layout_marginTopPercent="1.478743%w">

        <com.fimi.kernel.percent.PercentLinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <com.fimi.app.x8s.widget.X8ValueSeakBarView
                    android:id="@+id/vsb_speed_limit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    app:x8_value_accuracy="10"
                    app:x8_value_close_color="@color/white_100"
                    app:x8_value_open_color="@color/x8_fc_all_setting_blue"
                    app:x8_value_seekbar_default="10.0"
                    app:x8_value_seekbar_float="true"
                    app:x8_value_seekbar_max="18.0"
                    app:x8_value_seekbar_min="3.0"
                    app:x8_value_suffix="M/S"
                    app:x8_value_title="@string/x8_fc_item_speed_limit" />

                <com.fimi.app.x8s.widget.X8ValueSeakBarView
                    android:id="@+id/vsb_distance_limit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    app:x8_value_accuracy="10"
                    app:x8_value_close_color="@color/white_100"
                    app:x8_value_open_color="@color/x8_fc_all_setting_blue"
                    app:x8_value_seekbar_default="1000.0"
                    app:x8_value_seekbar_float="true"
                    app:x8_value_seekbar_max="5000.0"
                    app:x8_value_seekbar_min="10.0"
                    app:x8_value_suffix="M"
                    app:x8_value_title="@string/x8_fc_item_distance_limit" />

                <com.fimi.app.x8s.widget.X8ValueSeakBarView
                    android:id="@+id/vsb_height_limit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    app:x8_value_accuracy="10"
                    app:x8_value_close_color="@color/white_100"
                    app:x8_value_open_color="@color/x8_fc_all_setting_blue"
                    app:x8_value_seekbar_default="500.0"
                    app:x8_value_seekbar_float="true"
                    app:x8_value_seekbar_max="2000.0"
                    app:x8_value_seekbar_min="30.0"
                    app:x8_value_suffix="M"
                    app:x8_value_title="@string/x8_fc_item_height_limit" />

                <com.fimi.app.x8s.widget.X8ValueSeakBarView
                    android:id="@+id/vsb_return_height_limit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    app:x8_value_accuracy="10"
                    app:x8_value_close_color="@color/white_100"
                    app:x8_value_open_color="@color/x8_fc_all_setting_blue"
                    app:x8_value_seekbar_default="30.0"
                    app:x8_value_seekbar_float="true"
                    app:x8_value_seekbar_max="200.0"
                    app:x8_value_seekbar_min="30.0"
                    app:x8_value_suffix="M"
                    app:x8_value_title="@string/x8_fc_item_return_height_limit" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_novice_mode"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_novice_mode"
                        android:layout_width="@dimen/x8s_main_fc_item_x8"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x9_fc_item_sport_mode"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_sport_mode"
                        android:layout_width="@dimen/x8s_main_fc_item_x8"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_fc_item_x3"
                android:background="#00000000" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_magnetic_field_state"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ImageView
                        android:id="@+id/img_magnetic_field"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/x8_img_fc_magnetic_field" />
                </RelativeLayout>

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_compass_calibration"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <Button
                        android:id="@+id/btn_compass_calibration"
                        android:layout_width="wrap_content"
                        android:layout_height="0.0dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/x8_btn_fc_button_selector"
                        android:text="@string/x8_fc_item_calibration"
                        android:textAllCaps="false"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_36"
                        app:layout_heightPercent="59.597765%h" />
                </com.fimi.kernel.percent.PercentRelativeLayout>

                <com.fimi.app.x8s.widget.X8ValueSeakBarView
                    android:id="@+id/vsb_device_light"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:x8_value_close_color="@color/white_100"
                    app:x8_value_open_color="@color/x8_fc_all_setting_blue"
                    app:x8_value_seekbar_default="100.0"
                    app:x8_value_seekbar_max="100.0"
                    app:x8_value_seekbar_min="0.0"
                    app:x8_value_suffix="%"
                    app:x8_value_title="@string/x8_fc_item_device_light" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_fc_item_x3"
                android:background="#00000000" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_measure_for_disconnect"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/th_disconnect_measure"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_fc_item_x7"
                        android:layout_alignParentRight="true"
                        android:layout_centerInParent="true"
                        android:alpha="0.4"
                        android:padding="@dimen/x8s_main_fc_item_x1"
                        app:bg="#00000000"
                        app:default_index="1"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_fc_item_x1"
                        app:radiusC="@dimen/x8s_main_fc_item_x2"
                        app:src="@array/x8_disconnect_measure_state_option"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_fc_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
                </RelativeLayout>

                <com.fimi.kernel.percent.PercentRelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_return_point"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <LinearLayout
                        android:id="@+id/ll_return_person"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/x8_btn_return_selector"
                        android:padding="1.0dip">

                        <ImageButton
                            android:id="@+id/btn_return_person"
                            android:layout_width="80.0dip"
                            android:layout_height="wrap_content"
                            android:background="@android:color/transparent"
                            android:scaleType="centerInside"
                            android:src="@drawable/x8_btn_return_person_selector" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="4.0dip"
                        android:layout_toLeftOf="@+id/ll_return_person"
                        android:background="@drawable/x8_btn_return_selector"
                        android:padding="1.0dip">

                        <ImageButton
                            android:id="@+id/btn_return_drone"
                            android:layout_width="80.0dip"
                            android:layout_height="wrap_content"
                            android:background="@android:color/transparent"
                            android:scaleType="centerInside"
                            android:src="@drawable/x8_btn_return_drone_selector" />
                    </LinearLayout>
                </com.fimi.kernel.percent.PercentRelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_accurate_landing"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_accurate_landing"
                        android:layout_width="@dimen/x8s_main_fc_item_x8"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:id="@+id/tv_auto_set_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:drawableRight="@drawable/x8_img_auto_set_home"
                        android:gravity="center_vertical"
                        android:text="@string/x8_fc_item_auto_set_home"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_auto_set_home"
                        android:layout_width="@dimen/x8s_main_fc_item_x8"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x5">

                    <TextView
                        android:id="@+id/tv_set_follow_return"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:text="@string/x8_fc_item_follow_return"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_set_follow_return"
                        android:layout_width="@dimen/x8s_main_fc_item_x8"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_fc_item_x3"
                android:background="#00000000" />

            <LinearLayout
                android:id="@+id/ll_feeling_setting"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_rocker_exp_setting"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ImageView
                        android:id="@+id/fc_rocker_exp_setting"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/x8s_main_fc_item_x4"
                        android:paddingRight="@dimen/x8s_main_fc_item_x2"
                        android:scaleType="fitEnd"
                        android:src="@drawable/x8_param_goto_icon_seletor" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_fc_item_x9"
                    android:paddingLeft="@dimen/x8s_main_fc_item_x5"
                    android:paddingRight="@dimen/x8s_main_fc_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_fc_item_rocker_sensitivity_setting"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ImageView
                        android:id="@+id/fc_rocker_sensitivity_setting"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/x8s_main_fc_item_x4"
                        android:paddingRight="@dimen/x8s_main_fc_item_x2"
                        android:scaleType="fitEnd"
                        android:src="@drawable/x8_param_goto_icon_seletor" />
                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="20.0dip"
                android:background="#00000000" />

            <Button
                android:id="@+id/x8_fc_btn_rest_params"
                android:layout_width="428.0dip"
                android:layout_height="30.0dip"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/x8_gimbal_gain_selector"
                android:text="@string/x8_fc_reset_params"
                android:textAllCaps="false"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_36" />
        </com.fimi.kernel.percent.PercentLinearLayout>
    </ScrollView>
</com.fimi.kernel.percent.PercentRelativeLayout>