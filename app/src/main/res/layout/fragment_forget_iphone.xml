<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffffff"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_fi_title_sub_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33.299988dip"
        android:text="@string/login_password_retrieval"
        android:textColor="#cc000000"
        android:textSize="18.0dip" />

    <TextView
        android:id="@+id/tv_fi_select_country"
        android:layout_width="fill_parent"
        android:layout_height="43.299988dip"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="20.0dip"
        android:layout_marginTop="84.29999dip"
        android:layout_marginRight="20.0dip"
        android:background="@drawable/shape_et_register"
        android:gravity="center|left"
        android:inputType="number"
        android:textColor="#cc000000"
        android:textSize="12.0dip" />

    <EditText
        android:id="@+id/et_fi_account"
        android:layout_width="fill_parent"
        android:layout_height="43.299988dip"
        android:layout_below="@+id/tv_fi_select_country"
        android:layout_marginLeft="20.0dip"
        android:layout_marginTop="9.0dip"
        android:layout_marginRight="20.0dip"
        android:background="@drawable/shape_et_login_main_account"
        android:digits="\ 1234567890"
        android:hint="@string/login_hint_iphone"
        android:inputType="number"
        android:maxLength="11"
        android:textColor="#e6000000"
        android:textColorHint="#33000000"
        android:textCursorDrawable="@drawable/shape_login_cursor_color"
        android:textSize="12.0dip" />

    <View
        android:id="@+id/view1"
        android:layout_width="0.30000007dip"
        android:layout_height="16.599976dip"
        android:layout_alignTop="@+id/et_fi_account"
        android:layout_alignBottom="@+id/et_fi_account"
        android:layout_marginTop="13.299988dip"
        android:layout_marginBottom="13.299988dip"
        android:layout_toEndOf="@+id/tv_fi_area_code"
        android:background="#26000000" />

    <TextView
        android:id="@+id/tv_fi_area_code"
        android:layout_width="53.599976dip"
        android:layout_height="43.299988dip"
        android:layout_alignStart="@+id/et_fi_account"
        android:layout_alignTop="@+id/et_fi_account"
        android:gravity="center"
        android:text="+86"
        android:textColor="#cc000000"
        android:textSize="13.299988dip" />

    <EditText
        android:id="@+id/et_fi_verification"
        android:layout_width="fill_parent"
        android:layout_height="43.299988dip"
        android:layout_below="@+id/et_fi_account"
        android:layout_marginLeft="20.0dip"
        android:layout_marginTop="9.0dip"
        android:layout_marginRight="20.0dip"
        android:background="@drawable/shape_et_get_verfication"
        android:digits="\ 1234567890"
        android:hint="@string/login_hint_input_verification"
        android:inputType="number"
        android:maxLength="4"
        android:textColor="#e6000000"
        android:textColorHint="#33000000"
        android:textCursorDrawable="@drawable/shape_login_cursor_color"
        android:textSize="12.0dip" />

    <View
        android:id="@+id/view2"
        android:layout_width="0.30000007dip"
        android:layout_height="16.599976dip"
        android:layout_alignTop="@+id/et_fi_verification"
        android:layout_alignRight="@+id/et_fi_verification"
        android:layout_alignBottom="@+id/et_fi_verification"
        android:layout_marginTop="13.299988dip"
        android:layout_marginRight="87.0dip"
        android:layout_marginBottom="13.299988dip"
        android:background="#44000000" />

    <TextView
        android:id="@+id/btn_fi_get_validation_code"
        android:layout_width="87.0dip"
        android:layout_height="43.299988dip"
        android:layout_alignTop="@+id/et_fi_verification"
        android:layout_alignRight="@+id/et_fi_verification"
        android:layout_marginLeft="14.599976dip"
        android:clickable="true"
        android:gravity="center"
        android:text="@string/login_btn_verification"
        android:textColor="@color/selector_btn_register_get_verfication_code"
        android:textSize="13.0dip" />

    <TextView
        android:id="@+id/tv_fi_error_hint"
        android:layout_width="324.0dip"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_fi_verification"
        android:layout_alignLeft="@+id/et_fi_verification"
        android:layout_marginTop="11.299988dip"
        android:textColor="#ffff4a1a"
        android:textSize="12.659973dip"
        android:visibility="visible" />

    <EditText
        android:id="@+id/et_new_password"
        android:layout_width="fill_parent"
        android:layout_height="86.29999dip"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="20.0dip"
        android:layout_marginTop="84.29999dip"
        android:layout_marginRight="20.0dip"
        android:background="@drawable/shape_et_email_forget_pw"
        android:digits="@string/person_password_digts"
        android:gravity="center_vertical"
        android:hint="@string/login_input_new_password"
        android:inputType="textPassword"
        android:maxLength="16"
        android:textColor="#e6000000"
        android:textColorHint="#33000000"
        android:textCursorDrawable="@drawable/shape_login_cursor_color"
        android:textSize="12.0dip"
        android:visibility="invisible" />

    <View
        android:id="@+id/v_np_divider"
        android:layout_width="1.0px"
        android:layout_height="16.599976dip"
        android:layout_above="@+id/et_new_password_again"
        android:layout_alignTop="@+id/et_new_password"
        android:layout_alignRight="@+id/et_new_password"
        android:layout_marginTop="13.299988dip"
        android:layout_marginRight="53.599976dip"
        android:layout_marginBottom="13.299988dip"
        android:background="#26000000" />

    <ImageView
        android:id="@+id/iv_show_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/et_new_password_again"
        android:layout_alignTop="@+id/et_new_password"
        android:layout_alignRight="@+id/et_new_password"
        android:layout_marginRight="5.299988dip"
        android:src="@drawable/iv_login_email_password" />

    <ImageView
        android:id="@+id/iv_new_password_unified"
        android:layout_width="wrap_content"
        android:layout_height="34.299988dip"
        android:layout_above="@+id/et_new_password_again"
        android:layout_alignTop="@+id/et_new_password"
        android:layout_alignRight="@+id/et_new_password"
        android:layout_marginRight="21.0dip"
        android:src="@drawable/iv_fp_password_unified" />

    <EditText
        android:id="@+id/et_new_password_again"
        android:layout_width="fill_parent"
        android:layout_height="43.299988dip"
        android:layout_alignBottom="@+id/et_new_password"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="20.0dip"
        android:layout_marginRight="20.0dip"
        android:background="@drawable/shape_et_email_password_register"
        android:digits="@string/person_password_digts"
        android:hint="@string/login_input_new_password_again"
        android:inputType="textPassword"
        android:maxLength="16"
        android:textColor="#e6000000"
        android:textColorHint="#33000000"
        android:textCursorDrawable="@drawable/shape_login_cursor_color"
        android:textSize="12.0dip"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/tv_fi_password_error_hint"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_new_password_again"
        android:layout_alignLeft="@+id/et_new_password_again"
        android:layout_marginLeft="20.0dip"
        android:layout_marginTop="11.299988dip"
        android:layout_marginRight="20.0dip"
        android:textColor="#ffff4a1a"
        android:textSize="12.659973dip"
        android:visibility="visible" />

    <View
        android:id="@+id/v_divide"
        android:layout_width="313.29688dip"
        android:layout_height="1.0px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="127.5dip"
        android:background="#26000000" />

    <View
        android:id="@+id/v_np_again_divider"
        android:layout_width="1.0px"
        android:layout_height="16.599976dip"
        android:layout_alignTop="@+id/et_new_password_again"
        android:layout_alignRight="@+id/et_new_password_again"
        android:layout_alignBottom="@+id/et_new_password_again"
        android:layout_marginTop="13.299988dip"
        android:layout_marginRight="53.599976dip"
        android:layout_marginBottom="13.299988dip"
        android:background="#26000000" />

    <ImageView
        android:id="@+id/iv_show_password_again"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/et_new_password_again"
        android:layout_alignRight="@+id/et_new_password"
        android:layout_alignBottom="@+id/et_new_password_again"
        android:layout_marginRight="5.299988dip"
        android:src="@drawable/iv_login_email_password" />

    <ImageView
        android:id="@+id/iv_new_password_again_unified"
        android:layout_width="wrap_content"
        android:layout_height="34.299988dip"
        android:layout_alignTop="@+id/et_new_password_again"
        android:layout_alignRight="@+id/et_new_password_again"
        android:layout_alignBottom="@+id/et_new_password_again"
        android:layout_marginRight="21.0dip"
        android:src="@drawable/iv_fp_password_unified" />

    <Button
        android:id="@+id/btn_fi_login_phone"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_below="@+id/tv_fi_title_sub_name"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="244.0dip"
        android:background="@drawable/fimi_person_btn_bottom__shape_enable"
        android:text="@string/login_login_btn"
        android:textColor="@drawable/fimi_person_btn_butom_txcolor_select"
        android:textSize="13.299988dip"
        android:visibility="visible"
        app:layout_heightPercent="11.2962%w"
        app:layout_widthPercent="87.2222%w" />
</com.fimi.kernel.percent.PercentRelativeLayout>