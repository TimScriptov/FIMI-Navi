<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_rl_main_fc_item_drone_calibaration_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <RelativeLayout
        android:id="@+id/rl_drone_calibartion_title"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:background="@color/white_5"
        app:layout_heightPercent="14.814814%h"
        app:layout_marginLeftPercent="1.478743%w"
        app:layout_marginRightPercent="1.478743%w"
        app:layout_marginTopPercent="1.478743%w">

        <ImageView
            android:id="@+id/img_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/x8_img_return" />

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/x8_frequency_point_title"
            android:textColor="@color/white_100"
            android:textSize="@dimen/textsize_45" />
    </RelativeLayout>

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/rl_drone_calibartion_content"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/rl_drone_calibartion_title"
        android:background="@color/white_5"
        app:layout_marginBottomPercent="1.478743%w"
        app:layout_marginLeftPercent="1.478743%w"
        app:layout_marginRightPercent="1.478743%w"
        app:layout_marginTopPercent="1.11111%w">

        <ScrollView
            android:id="@+id/sv_ai_items"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:fadingEdge="none"
            android:overScrollMode="never"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <TextView
                        android:id="@+id/tv_signal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/v_fre_point"
                        android:layout_marginTop="15.0dip"
                        android:text="@string/x8_frequency_point_signal"
                        android:textColor="@color/white_80"
                        android:textSize="11.0dip" />

                    <com.fimi.app.x8s.widget.X8FrequencyPoint
                        android:id="@+id/v_fre_point"
                        android:layout_width="380.0dip"
                        android:layout_height="101.0dip"
                        android:layout_below="@+id/tv_signal"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="11.0dip" />

                    <TextView
                        android:id="@+id/tv_point"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_signal"
                        android:layout_marginLeft="7.0dip"
                        android:layout_marginTop="101.0dip"
                        android:layout_toRightOf="@+id/v_fre_point"
                        android:text="@string/x8_frequency_point"
                        android:textColor="@color/white_80"
                        android:textSize="11.0dip" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="101.0dip"
                        android:layout_below="@+id/tv_signal"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="11.0dip"
                        android:layout_marginRight="6.0dip"
                        android:layout_toLeftOf="@+id/v_fre_point"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="0.0dip"
                            android:layout_weight="1.0"
                            android:gravity="center"
                            android:text="@string/x8_frequency_point_signal_h"
                            android:textColor="@color/white_80"
                            android:textSize="10.0dip" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="0.0dip"
                            android:layout_weight="1.0"
                            android:gravity="center"
                            android:text="@string/x8_frequency_point_signal_m"
                            android:textColor="@color/white_80"
                            android:textSize="10.0dip" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="0.0dip"
                            android:layout_weight="1.0"
                            android:gravity="center"
                            android:text="@string/x8_frequency_point_signal_l"
                            android:textColor="@color/white_80"
                            android:textSize="10.0dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lv_select"
                        android:layout_width="312.0dip"
                        android:layout_height="20.0dip"
                        android:layout_below="@+id/tv_point"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="15.0dip"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="0.0dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0">

                            <Button
                                android:id="@+id/tv_point1"
                                android:layout_width="47.0dip"
                                android:layout_height="fill_parent"
                                android:layout_centerInParent="true"
                                android:background="@drawable/x8_fre_point_selector"
                                android:clickable="true"
                                android:gravity="center"
                                android:text="1"
                                android:textColor="@drawable/x8_fre_point_text_selector"
                                android:textSize="13.0dip" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0.0dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0">

                            <Button
                                android:id="@+id/tv_point2"
                                android:layout_width="47.0dip"
                                android:layout_height="fill_parent"
                                android:layout_centerInParent="true"
                                android:background="@drawable/x8_fre_point_selector"
                                android:clickable="true"
                                android:gravity="center"
                                android:text="2"
                                android:textColor="@drawable/x8_fre_point_text_selector"
                                android:textSize="13.0dip" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0.0dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0">

                            <Button
                                android:id="@+id/tv_point3"
                                android:layout_width="47.0dip"
                                android:layout_height="fill_parent"
                                android:layout_centerInParent="true"
                                android:background="@drawable/x8_fre_point_selector"
                                android:clickable="true"
                                android:gravity="center"
                                android:text="3"
                                android:textColor="@drawable/x8_fre_point_text_selector"
                                android:textSize="13.0dip" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0.0dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0">

                            <Button
                                android:id="@+id/tv_point4"
                                android:layout_width="47.0dip"
                                android:layout_height="fill_parent"
                                android:layout_centerInParent="true"
                                android:background="@drawable/x8_fre_point_selector"
                                android:clickable="true"
                                android:gravity="center"
                                android:text="4"
                                android:textColor="@drawable/x8_fre_point_text_selector"
                                android:textSize="13.0dip" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0.0dip"
                            android:layout_height="fill_parent"
                            android:layout_weight="1.0">

                            <Button
                                android:id="@+id/tv_point5"
                                android:layout_width="47.0dip"
                                android:layout_height="fill_parent"
                                android:layout_centerInParent="true"
                                android:background="@drawable/x8_fre_point_selector"
                                android:clickable="true"
                                android:gravity="center"
                                android:text="5"
                                android:textColor="@drawable/x8_fre_point_text_selector"
                                android:textSize="13.0dip" />
                        </RelativeLayout>
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/lv_select"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="24.0dip"
                        android:gravity="center"
                        android:text="@string/x8_frequency_point_tip"
                        android:textColor="#ffcccccc"
                        android:textSize="10.0dip" />
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </com.fimi.kernel.percent.PercentRelativeLayout>
</com.fimi.kernel.percent.PercentRelativeLayout>