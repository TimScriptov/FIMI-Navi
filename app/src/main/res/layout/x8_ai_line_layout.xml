<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_x8_ai_surround"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <ImageView
        android:id="@+id/img_ai_follow_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:background="@drawable/x8_btn_ai_follow_point2point_next"
        app:layout_marginRightPercent="4.6666%h"
        app:layout_marginTopPercent="8.3333%" />

    <RelativeLayout
        android:id="@+id/rl_ai_line_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_marginLeftPercent="4.1666%h"
        app:layout_marginTopPercent="37.5%h">

        <com.fimi.app.x8s.widget.TwoStateImageView
            android:id="@+id/img_ai_line_add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/x8_img_ai_line_add_selector" />

        <TextView
            android:id="@+id/tv_ai_line_index"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@color/white_100"
            android:textSize="@dimen/textsize_39" />
    </RelativeLayout>

    <com.fimi.app.x8s.widget.TwoStateImageView
        android:id="@+id/img_ai_line_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/x8_img_ai_line_delete_selector"
        app:layout_marginLeftPercent="4.1666%h"
        app:layout_marginTopPercent="53.6111%h" />

    <com.fimi.app.x8s.widget.TwoStateImageView
        android:id="@+id/img_ai_line_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/x8_btn_ai_line_point_value_edit"
        app:layout_marginLeftPercent="4.1666%h"
        app:layout_marginTopPercent="37.5%h" />

    <com.fimi.app.x8s.widget.TwoStateImageView
        android:id="@+id/img_ai_line_history"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/x8_btn_ai_line_go_history"
        android:visibility="gone"
        app:layout_marginLeftPercent="4.1666%h"
        app:layout_marginTopPercent="37.5%h" />

    <TextView
        android:id="@+id/tv_tip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/x8_ai_surround_tx_bg2"
        android:paddingLeft="@dimen/x8s_ai_line_layout_x7"
        android:paddingTop="@dimen/x8s_ai_line_layout_x2"
        android:paddingRight="31.0dip"
        android:paddingBottom="@dimen/x8s_ai_line_layout_x2"
        android:text="@string/x8_ai_fly_lines_interest_point"
        android:textColor="@color/white_100"
        android:visibility="gone"
        app:layout_marginBottomPercent="19.7222%h" />

    <View
        android:id="@+id/v_divide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="7.0dip"
        android:layout_toRightOf="@+id/tv_tip"
        app:layout_marginBottomPercent="19.7222%h" />

    <ImageView
        android:id="@+id/img_interest_point"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/v_divide"
        android:background="@drawable/x8_img_ai_line_inreterst_small"
        android:visibility="gone"
        app:layout_marginBottomPercent="19.7222%h" />

    <com.fimi.app.x8s.widget.X8AiTipWithCloseView
        android:id="@+id/v_content_tip"
        android:layout_width="wrap_content"
        android:layout_height="50.0dip"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal"
        app:layout_marginBottomPercent="4.722222%" />

    <TextView
        android:id="@+id/rl_action_tip1"
        android:layout_width="wrap_content"
        android:layout_height="27.0dip"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/x8_ai_follow_marker_info_bg"
        android:gravity="center"
        android:paddingLeft="12.0dip"
        android:paddingRight="12.0dip"
        android:textColor="@color/white_100"
        android:visibility="gone"
        app:layout_marginBottomPercent="15.83%" />

    <ImageView
        android:id="@+id/img_vc_targgle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="15.0dip"
        android:background="@drawable/x8_btn_ai_vc_targgle"
        android:visibility="gone"
        app:layout_marginBottomPercent="20.833333%" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        app:layout_marginBottomPercent="5.833333%"
        app:layout_marginRightPercent="4.6666%h">

        <RelativeLayout
            android:id="@+id/rl_flag_small"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/x8_ai_follow_tx_bg"
            android:paddingRight="11.0dip">

            <com.fimi.app.x8s.widget.TwoStateImageView
                android:id="@+id/img_ai_flag_small"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:background="@drawable/x8_img_ai_line_small" />

            <TextView
                android:id="@+id/img_ai_p2p_tip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/img_ai_flag_small"
                android:gravity="center_vertical"
                android:text="@string/x8_ai_fly_route"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />
        </RelativeLayout>

        <com.fimi.app.x8s.widget.TwoStateImageView
            android:id="@+id/img_ai_follow_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13.0dip"
            android:layout_toRightOf="@+id/rl_flag_small"
            android:background="@drawable/x8_btn_ai_follow_point2point_back" />
    </RelativeLayout>

    <View
        android:id="@+id/x8_main_ai_line_next_blank"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_toLeftOf="@+id/x8_main_ai_follow_confirm_main_layout_content"
        android:visibility="gone" />

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/x8_main_ai_line_point_value_content"
        android:layout_width="0.0dip"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:background="@color/black_80"
        android:visibility="gone"
        app:layout_widthPercent="50%w" />
</com.fimi.kernel.percent.PercentRelativeLayout>