<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_rl_main_general_item"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="none"
        android:overScrollMode="never"
        android:scrollbars="none"
        app:layout_marginBottomPercent="1.478743%w"
        app:layout_marginLeftPercent="1.478743%w"
        app:layout_marginRightPercent="1.478743%w"
        app:layout_marginTopPercent="1.478743%w">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/x8_ll_flightlog"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_flightlog"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/x8s_main_fc_item_x4"
                        android:paddingRight="@dimen/x8s_main_fc_item_x2"
                        android:scaleType="fitEnd"
                        android:src="@drawable/x8_param_goto_icon_seletor" />
                </com.fimi.kernel.percent.PercentRelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_general_item_x3"
                android:background="#00000000" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_general_map"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/th_map"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_general_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerInParent="true"
                        android:padding="@dimen/x8s_main_general_item_x1"
                        app:bg="#00000000"
                        app:default_index="1"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_general_item_x1"
                        app:radiusC="@dimen/x8s_main_general_item_x2"
                        app:src="@array/x8_general_map_option"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_general_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
                </com.fimi.kernel.percent.PercentRelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_general_map_rectify_deviation"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_map_rectify_deviation"
                        android:layout_width="@dimen/x8s_main_general_item_x7"
                        android:layout_height="@dimen/x8s_main_general_item_x5"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_general_map_switch"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_google_map"
                        android:layout_width="@dimen/x8s_main_general_item_x7"
                        android:layout_height="@dimen/x8s_main_general_item_x5"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_general_item_x3"
                android:background="#00000000" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_general_unity"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/th_unity"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_general_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerInParent="true"
                        android:padding="@dimen/x8s_main_general_item_x1"
                        app:bg="#00000000"
                        app:default_index="1"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_general_item_x1"
                        app:radiusC="@dimen/x8s_main_general_item_x2"
                        app:src="@array/x8_general_unity_option"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_general_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
                </com.fimi.kernel.percent.PercentRelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_general_item_x3"
                android:background="#00000000"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_general_show_log"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.widget.SwitchButton
                        android:id="@+id/swb_show_log"
                        android:layout_width="@dimen/x8s_main_general_item_x7"
                        android:layout_height="@dimen/x8s_main_general_item_x5"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        app:onColor="#ff09d9ff" />
                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_general_item_x3"
                android:background="#00000000" />

            <LinearLayout
                android:id="@+id/ll_version"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:id="@+id/rl_update"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_general_version_firmware_upgrade"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/x8s_main_fc_item_x4"
                        android:paddingRight="@dimen/x8s_main_fc_item_x2"
                        android:scaleType="fitEnd"
                        android:src="@drawable/x8_param_goto_icon_seletor" />
                </com.fimi.kernel.percent.PercentRelativeLayout>

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_general_version_rest_paramters"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ProgressBar
                        android:id="@+id/pb_restsystem_loading"
                        android:layout_width="0.0dip"
                        android:layout_height="0.0dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerInParent="true"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:indeterminateDrawable="@drawable/x8_progress_load"
                        android:visibility="gone"
                        app:layout_heightPercent="59.597765%h"
                        app:layout_widthPercent="59.597765%h" />

                    <Button
                        android:id="@+id/btn_rest_params"
                        android:layout_width="wrap_content"
                        android:layout_height="0.0dip"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/x8_btn_fc_button_selector"
                        android:text="@string/x8_general_rest"
                        android:textAllCaps="false"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_36"
                        app:layout_heightPercent="59.597765%h" />
                </com.fimi.kernel.percent.PercentRelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_general_item_x3"
                android:background="#00000000" />

            <LinearLayout
                android:id="@+id/ll_modify"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical">

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_modify_mode"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/x8s_main_fc_item_x4"
                        android:paddingRight="@dimen/x8s_main_fc_item_x2"
                        android:scaleType="fitEnd"
                        android:src="@drawable/x8_param_goto_icon_seletor" />
                </com.fimi.kernel.percent.PercentRelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/x8s_main_general_item_x3"
                android:background="#00000000"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/ll_frequency_point"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/white_5"
                android:orientation="vertical"
                android:visibility="gone">

                <com.fimi.kernel.percent.PercentRelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_main_general_item_x8"
                    android:paddingLeft="@dimen/x8s_main_general_item_x4"
                    android:paddingRight="@dimen/x8s_main_general_item_x4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/x8_frequency_point"
                        android:textColor="@color/white_100"
                        android:textSize="@dimen/textsize_39" />

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/x8s_main_fc_item_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/x8s_main_fc_item_x4"
                        android:paddingRight="@dimen/x8s_main_fc_item_x2"
                        android:scaleType="fitEnd"
                        android:src="@drawable/x8_param_goto_icon_seletor" />
                </com.fimi.kernel.percent.PercentRelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</com.fimi.kernel.percent.PercentRelativeLayout>