<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_rl_main_mdify_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/layout_top"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:background="@color/white_5"
        app:layout_heightPercent="14.814815%h"
        app:layout_marginLeftPercent="1.478743%w"
        app:layout_marginRightPercent="1.478743%w"
        app:layout_marginTopPercent="1.478743%h">

        <ImageView
            android:id="@+id/btn_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/x8_img_return" />

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/x8_modify_mode"
            android:textColor="@color/white_100"
            android:textSize="@dimen/textsize_45" />
    </com.fimi.kernel.percent.PercentRelativeLayout>

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/x8_modify_item_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layout_top"
        app:layout_marginLeftPercent="1.478743%w"
        app:layout_marginRightPercent="1.478743%w">

        <RelativeLayout
            android:id="@+id/x8_sensor_layout"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_main_general_item_modify_layout_x2"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/sensor_title"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@android:color/transparent"
                android:gravity="center|left"
                android:paddingLeft="@dimen/x8s_main_general_item_modify_layout_x1"
                android:text="@string/x8_modify_sensor"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_main_general_item_modify_layout_x1"
                android:src="@drawable/x8_param_goto_icon_seletor" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/x8_gimbal_sensor_layout"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_main_general_item_modify_layout_x2"
            android:layout_below="@+id/x8_sensor_layout"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/gimbal_sensor_title"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@android:color/transparent"
                android:gravity="center|left"
                android:paddingLeft="@dimen/x8s_main_general_item_modify_layout_x1"
                android:text="@string/x8_modify_gimbal_sensor"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_main_general_item_modify_layout_x1"
                android:src="@drawable/x8_param_goto_icon_seletor" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/x8_blackbox_layout"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_main_general_item_modify_layout_x2"
            android:layout_below="@+id/x8_gimbal_sensor_layout"
            android:gravity="center"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/gyro_title"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@android:color/transparent"
                android:gravity="center|left"
                android:paddingLeft="@dimen/x8s_main_general_item_modify_layout_x1"
                android:text="@string/x8_modify_black_bor"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_main_general_item_modify_layout_x1"
                android:src="@drawable/x8_param_goto_icon_seletor" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/x8_check_layout"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_main_general_item_modify_layout_x2"
            android:layout_below="@+id/x8_blackbox_layout"
            android:gravity="center"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/gyro2_title"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center|left"
                android:paddingLeft="@dimen/x8s_main_general_item_modify_layout_x1"
                android:text="@string/x8_modify_check"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_main_general_item_modify_layout_x1"
                android:src="@drawable/x8_param_goto_icon_seletor" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/x8_blackbox_layout1"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_main_general_item_modify_layout_x2"
            android:layout_below="@+id/x8_check_layout"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_b2box_title"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@android:color/transparent"
                android:gravity="center|left"
                android:paddingLeft="@dimen/x8s_main_general_item_modify_layout_x1"
                android:text="@string/x8_modify_black_box"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_main_general_item_modify_layout_x1"
                android:src="@drawable/x8_param_goto_icon_seletor" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/x8_format_storage_layout"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_main_general_item_modify_layout_x2"
            android:layout_below="@+id/x8_blackbox_layout1"
            android:gravity="center"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/tv_format_storage_title"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@android:color/transparent"
                android:gravity="center|left"
                android:paddingLeft="@dimen/x8s_main_general_item_modify_layout_x1"
                android:text="@string/x8_modify_format_storage"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />

            <ImageView
                android:id="@+id/iv_format_storage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x8s_main_general_item_modify_layout_x1"
                android:src="@drawable/x8_param_goto_icon_seletor" />

            <TextView
                android:id="@+id/tv_format_storage_dec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/pb_format_storage"
                android:text="@string/x8_modify_format_storage_prohibit"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39"
                android:visibility="gone" />

            <ProgressBar
                android:id="@+id/pb_format_storage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:indeterminateDrawable="@drawable/progress_load"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39"
                android:visibility="gone" />
        </RelativeLayout>

        <com.fimi.kernel.percent.PercentRelativeLayout
            android:id="@+id/x8_aircraft_calibration_layout"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/x8s_main_general_item_x8"
            android:layout_below="@+id/x8_format_storage_layout"
            android:paddingLeft="@dimen/x8s_main_general_item_x4"
            android:paddingRight="@dimen/x8s_main_general_item_x4"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/x8_modify_aircraft_calibration"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_39" />

            <Button
                android:id="@+id/x8_btn_calibration"
                android:layout_width="wrap_content"
                android:layout_height="0.0dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/x8_btn_fc_button_selector"
                android:text="@string/x8_rc_setting_calibration"
                android:textAllCaps="false"
                android:textColor="@color/white_100"
                android:textSize="@dimen/textsize_36"
                app:layout_heightPercent="59.597765%h" />
        </com.fimi.kernel.percent.PercentRelativeLayout>
    </com.fimi.kernel.percent.PercentRelativeLayout>

    <ViewStub
        android:id="@+id/x8_calibration_view"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout="@layout/x8_main_general_item_aircraft_calibration_layout" />

    <ViewStub
        android:id="@+id/x8_sensor_view"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout="@layout/x8_main_general_item_modify_sensor_layout" />

    <ViewStub
        android:id="@+id/x8_gimbal_sensor_view"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout="@layout/x8_main_general_item_modify_gimbal_sensor_layout" />

    <RelativeLayout
        android:id="@+id/rl_next_content"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layout_top"
        app:layout_marginLeftPercent="1.478743%w"
        app:layout_marginRightPercent="1.478743%w" />
</com.fimi.kernel.percent.PercentRelativeLayout>