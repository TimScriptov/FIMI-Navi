<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:doubleWaySeekBar="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/x8_gimbal_horizontal_help_line" />

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/rl_gc_calibrate"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_alignParentRight="true"
        android:background="@color/black_80"
        android:orientation="vertical"
        doubleWaySeekBar:layout_heightPercent="74.16%"
        doubleWaySeekBar:layout_marginTopPercent="8.05%"
        doubleWaySeekBar:layout_widthPercent="29.06%">

        <com.fimi.kernel.percent.PercentLinearLayout
            android:id="@+id/x8_pl_xyz_adjust_setting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.fimi.app.x8s.widget.X8GimbalXYZAdjustRelayout
                android:id="@+id/x8_view_gimbal_p"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true" />

            <com.fimi.app.x8s.widget.X8GimbalXYZAdjustRelayout
                android:id="@+id/x8_view_gimbal_r"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true" />

            <com.fimi.app.x8s.widget.X8GimbalXYZAdjustRelayout
                android:id="@+id/x8_view_gimbal_y"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true" />
        </com.fimi.kernel.percent.PercentLinearLayout>

        <com.fimi.kernel.percent.PercentLinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            doubleWaySeekBar:layout_marginBottomPercent="2.77%">

            <Button
                android:id="@+id/x8_btn_xyz_adjust_exit"
                android:layout_width="67.0dip"
                android:layout_height="27.0dip"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/x8_gimbal_xyz_adjust_btn_exit_selector"
                android:gravity="center"
                android:text="@string/x8_gimbal_xyz_adjust_signout"
                android:textAllCaps="false"
                android:textColor="@color/white_100"
                android:textSize="12.0dip" />

            <Button
                android:id="@+id/x8_btn_xyz_adjust_save"
                android:layout_width="67.0dip"
                android:layout_height="27.0dip"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/x8_gimbal_xyz_adjust_btn_selector"
                android:gravity="center"
                android:text="@string/x8_save"
                android:textAllCaps="false"
                android:textColor="@color/x8_gimbal_xyz_adjust_txt"
                android:textSize="12.0dip"
                doubleWaySeekBar:layout_marginLeftPercent="15.9%" />
        </com.fimi.kernel.percent.PercentLinearLayout>
        ">
        "
    </com.fimi.kernel.percent.PercentRelativeLayout>

    <com.fimi.app.x8s.widget.X8AiTipWithCloseView
        android:id="@+id/x8_gimbal_xyz_adiust_tip"
        android:layout_width="wrap_content"
        android:layout_height="50.0dip"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal"
        doubleWaySeekBar:layout_marginBottomPercent="4.722222%" />
</com.fimi.kernel.percent.PercentRelativeLayout>