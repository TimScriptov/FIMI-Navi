<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_ai_point_to_point_confirm_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentRight="true"
    android:clickable="true">

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/rl_ai_auto_photo_confirm"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        app:layout_heightPercent="11%">

        <ImageView
            android:id="@+id/img_ai_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/x8_img_return"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_ai_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/x8_ai_auto_photo_title"
            android:textColor="@color/white_90"
            android:textSize="@dimen/textsize_45" />
    </com.fimi.kernel.percent.PercentRelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:background="@color/white_10"
        app:layout_marginTopPercent="11.11111%" />

    <TextView
        android:id="@+id/tv_ai_next_content1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/x8_ai_auto_photo_vertical_next_tip1"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginLeftPercent="5.8333333%"
        app:layout_marginTopPercent="14.4444%" />

    <TextView
        android:id="@+id/tv_ai_time_lable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_ai_next_content1"
        android:text="@string/x8_ai_fly_follow_point_to_point_tip5"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginLeftPercent="5.8333333%"
        app:layout_marginTopPercent="2.777777%" />

    <TextView
        android:id="@+id/tv_ai_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_ai_next_content1"
        android:layout_marginLeft="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x4"
        android:layout_toRightOf="@+id/tv_ai_time_lable"
        android:text="10S"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginTopPercent="2.777777%" />

    <TextView
        android:id="@+id/tv_ai_speed_lable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_ai_time_lable"
        android:text="@string/x8_ai_fly_follow_point_to_point_tip6"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginLeftPercent="5.8333333%"
        app:layout_marginTopPercent="2.777777%" />

    <TextView
        android:id="@+id/tv_ai_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_ai_time_lable"
        android:layout_marginLeft="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x4"
        android:layout_toRightOf="@+id/tv_ai_distance_lable"
        android:text="3M/S"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginTopPercent="2.777777%" />

    <RelativeLayout
        android:id="@+id/rl_seekbar"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x11"
        android:layout_below="@+id/tv_ai_speed_lable">

        <RelativeLayout
            android:id="@+id/rl_speed_minus"
            android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

            <View
                android:id="@+id/view_minus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x3"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x1"
                android:layout_centerInParent="true"
                android:background="@drawable/x8_btn_minus_bg" />
        </RelativeLayout>

        <com.fimi.app.x8s.widget.X8SeekBarView
            android:id="@+id/sb_speed"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/rl_speed_plus"
            android:layout_toRightOf="@+id/rl_speed_minus" />

        <RelativeLayout
            android:id="@+id/rl_speed_plus"
            android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

            <View
                android:id="@+id/view_plus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                android:layout_centerInParent="true"
                android:background="@drawable/x8_btn_plus_bg" />
        </RelativeLayout>
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_ai_distance_lable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_seekbar"
        android:text="@string/x8_ai_fly_follow_point_to_point_tip4"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginLeftPercent="5.8333333%" />

    <TextView
        android:id="@+id/tv_ai_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_seekbar"
        android:layout_marginLeft="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x4"
        android:layout_toRightOf="@+id/tv_ai_distance_lable"
        android:text="30M"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39" />

    <RelativeLayout
        android:id="@+id/rl_distance_seekbar"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x11"
        android:layout_below="@+id/tv_ai_distance_lable">

        <RelativeLayout
            android:id="@+id/rl_distance_minus"
            android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

            <View
                android:id="@+id/view_distance_minus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x3"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x1"
                android:layout_centerInParent="true"
                android:background="@drawable/x8_btn_minus_bg" />
        </RelativeLayout>

        <com.fimi.app.x8s.widget.X8SeekBarView
            android:id="@+id/sb_distance"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/rl_distance_plus"
            android:layout_toRightOf="@+id/rl_distance_minus" />

        <RelativeLayout
            android:id="@+id/rl_distance_plus"
            android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x6"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8">

            <View
                android:id="@+id/view_distance_plus"
                android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x5"
                android:layout_centerInParent="true"
                android:background="@drawable/x8_btn_plus_bg" />
        </RelativeLayout>
    </RelativeLayout>

    <TextView
        android:id="@+id/tv_ai_auto_return"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_distance_seekbar"
        android:text="@string/x8_ai_auto_photo_vertical_next_tip2"
        android:textColor="@color/white_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginLeftPercent="5.8333333%" />

    <com.fimi.widget.SwitchButton
        android:id="@+id/swb_ai_auto_return"
        android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x10"
        android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x7"
        android:layout_below="@+id/rl_distance_seekbar"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x8"
        app:layout_marginTopPercent="2.222%"
        app:onColor="#ff09d9ff" />

    <Button
        android:id="@+id/btn_ai_confirm_ok"
        android:layout_width="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x12"
        android:layout_height="@dimen/x8s_ai_auto_photo_excute_confirm_layout_x9"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/x8_btn_white_selector"
        android:text="@string/x8_ai_fly_follow_go"
        android:textColor="@color/black_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginBottomPercent="2.777777%" />
</com.fimi.kernel.percent.PercentRelativeLayout>