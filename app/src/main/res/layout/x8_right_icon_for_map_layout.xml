<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x8s_main_right_menu_layout_x4"
        android:layout_alignParentRight="true"
        app:layout_marginTopPercent="15.833%h">

        <com.fimi.app.x8s.widget.TwoStateImageButton
            android:id="@+id/imb_location"
            android:layout_width="@dimen/x8s_main_right_menu_layout_x4"
            android:layout_height="@dimen/x8s_main_right_menu_layout_x4"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x8s_main_right_menu_layout_x2"
            android:background="@drawable/x8_main_location_selector" />

        <LinearLayout
            android:id="@+id/ll_set_home_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="15.0dip"
            android:layout_toLeftOf="@+id/imb_location"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/x8_tab_left_bg">

                <com.fimi.app.x8s.widget.TwoStateImageButton
                    android:id="@+id/img_set_home_by_dv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/x8_btn_set_home_point_by_dv" />

            </LinearLayout>

            <View
                android:layout_width="1.0dip"
                android:layout_height="fill_parent" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/x8_tab_right_bg">

                <com.fimi.app.x8s.widget.TwoStateImageButton
                    android:id="@+id/img_set_home_by_man"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/x8_btn_set_home_point_by_man" />

            </LinearLayout>

        </LinearLayout>

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_takeoff_landing_aifly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:orientation="horizontal"
        app:layout_marginTopPercent="86%h">

        <com.fimi.app.x8s.widget.TwoStateImageButton
            android:id="@+id/imb_x8_ai_reture"
            android:layout_width="32.0dip"
            android:layout_height="32.0dip"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x8s_main_right_menu_layout_x3"
            android:background="@drawable/x8_btn_ai_small_return_home" />

        <com.fimi.app.x8s.widget.TwoStateImageButton
            android:id="@+id/imb_x8_take_off_land"
            android:layout_width="32.0dip"
            android:layout_height="32.0dip"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x8s_main_right_menu_layout_x3"
            android:background="@drawable/x8_btn_ai_small_takeoff" />

        <com.fimi.app.x8s.widget.TwoStateImageButton
            android:id="@+id/imb_ai_fly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x8s_main_right_menu_layout_x3"
            android:background="@drawable/x8_main_intelligent_flight_select" />

    </LinearLayout>

</merge>
