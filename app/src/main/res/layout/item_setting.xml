<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl"
    android:layout_width="fill_parent"
    android:layout_height="@dimen/x8s_item_setting_x3"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/setting_arrow"
        android:layout_width="@dimen/x8s_item_setting_x1"
        android:layout_height="@dimen/x8s_item_setting_x2"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:background="@drawable/x8_img_return"
        android:visibility="gone"
        app:layout_marginLeftPercent="1.71%" />

    <TextView
        android:id="@+id/setting_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#4dffffff"
        android:visibility="gone"
        app:layout_marginLeftPercent="46.05%h"
        app:layout_marginTopPercent="49.34%h"
        app:layout_textSizePercent="23.7%h" />

    <TextView
        android:id="@+id/settig_coontent_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/setting_title_tv"
        android:textColor="#e6ffffff"
        android:visibility="gone"
        app:layout_marginLeftPercent="46.05%h"
        app:layout_textSizePercent="27.6%h" />

    <com.fimi.widget.SwitchButton
        android:id="@+id/switch_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:visibility="gone"
        app:layout_heightPercent="39.47%h"
        app:layout_marginRightPercent="46.05%h"
        app:layout_widthPercent="70.39%h" />
</com.fimi.kernel.percent.PercentRelativeLayout>