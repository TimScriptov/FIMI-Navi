<?xml version="1.0" encoding="utf-8"?>
<com.fimi.kernel.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/x8_ai_point_to_point_confirm_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentRight="true"
    android:clickable="true">

    <com.fimi.kernel.percent.PercentRelativeLayout
        android:id="@+id/rl_ai_follow_confirm"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        app:layout_heightPercent="11%">

        <ImageView
            android:id="@+id/img_ai_follow_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/x8_img_return"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_ai_follow_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/x8_ai_fly_route"
            android:textColor="@color/white_90"
            android:textSize="@dimen/textsize_45" />
    </com.fimi.kernel.percent.PercentRelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:background="@color/white_10"
        app:layout_marginTopPercent="11.11111%" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:fadingEdge="none"
        android:overScrollMode="never"
        android:scrollbars="none"
        app:layout_heightPercent="65%"
        app:layout_marginTopPercent="14%">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView
                    android:id="@+id/tv_ai_follow_size_lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16.0dip"
                    android:text="@string/x8_ai_fly_lines_point_size"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_follow_size"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/x8s_ai_line_excute_confirm_layout_x4"
                    android:layout_toRightOf="@+id/tv_ai_follow_size_lable"
                    android:text="3"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_follow_confirm_title1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_follow_size_lable"
                    android:layout_marginLeft="16.0dip"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/x8_ai_fly_lines_distance"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_follow_distance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_follow_size_lable"
                    android:layout_marginLeft="@dimen/x8s_ai_line_excute_confirm_layout_x4"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@+id/tv_ai_follow_confirm_title1"
                    android:text="30M"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_follow_confirm_title2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_follow_confirm_title1"
                    android:layout_marginLeft="16.0dip"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/x8_ai_fly_follow_point_to_point_tip5"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_follow_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_follow_confirm_title1"
                    android:layout_marginLeft="@dimen/x8s_ai_line_excute_confirm_layout_x4"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@+id/tv_ai_follow_confirm_title2"
                    android:text="10S"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_follow_confirm_title3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_follow_confirm_title2"
                    android:layout_marginLeft="16.0dip"
                    android:layout_marginTop="10.0dip"
                    android:text="@string/x8_ai_fly_follow_point_to_point_tip6"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <TextView
                    android:id="@+id/tv_ai_follow_speed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_follow_confirm_title2"
                    android:layout_marginLeft="@dimen/x8s_ai_line_excute_confirm_layout_x4"
                    android:layout_marginTop="10.0dip"
                    android:layout_toRightOf="@+id/tv_ai_follow_confirm_title3"
                    android:text="3M/S"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39" />

                <RelativeLayout
                    android:id="@+id/rl_seekbar"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x11"
                    android:layout_below="@+id/tv_ai_follow_confirm_title3"
                    android:layout_marginTop="10.0dip">

                    <RelativeLayout
                        android:id="@+id/rl_minus"
                        android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x6"
                        android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x6"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x8s_ai_line_excute_confirm_layout_x8">

                        <View
                            android:id="@+id/view_minus"
                            android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x3"
                            android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x1"
                            android:layout_centerInParent="true"
                            android:background="@drawable/x8_btn_minus_bg" />
                    </RelativeLayout>

                    <SeekBar
                        android:id="@+id/sb_value"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x8s_ai_line_excute_confirm_layout_x5"
                        android:layout_marginRight="@dimen/x8s_ai_line_excute_confirm_layout_x5"
                        android:layout_toLeftOf="@+id/rl_plus"
                        android:layout_toRightOf="@+id/rl_minus"
                        android:background="@null"
                        android:focusable="true"
                        android:maxHeight="@dimen/x8s_ai_line_excute_confirm_layout_x2"
                        android:minHeight="@dimen/x8s_ai_line_excute_confirm_layout_x2"
                        android:progressDrawable="@drawable/x8_img_seekbar_bg"
                        android:thumb="@drawable/x8_img_seekbar_thum" />

                    <RelativeLayout
                        android:id="@+id/rl_plus"
                        android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x6"
                        android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x6"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/x8s_ai_line_excute_confirm_layout_x8">

                        <View
                            android:id="@+id/view_plus"
                            android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x5"
                            android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x5"
                            android:layout_centerInParent="true"
                            android:background="@drawable/x8_btn_plus_bg" />
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_ai_line_curve_videotape"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_seekbar"
                    android:layout_marginTop="14.0dip"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/tv_ai_line_curve_videotape"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="16.0dip"
                        android:layout_marginTop="8.0dip"
                        android:text="@string/x8_ai_fly_gravitation_excute_videotape_whole"
                        android:textColor="@color/white_90"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/x8_ai_line_curve_videotape"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_fc_item_x7"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="6.0dip"
                        android:padding="@dimen/x8s_main_fc_item_x1"
                        app:bg="#00000000"
                        app:default_index="0"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_fc_item_x1"
                        app:radiusC="@dimen/x8s_main_fc_item_x2"
                        app:src="@array/x8_ai_gravitation_excute_value_videotape"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_fc_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_rorate"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_ai_line_curve_videotape"
                    android:layout_marginTop="14.0dip">

                    <TextView
                        android:id="@+id/tv_ai_orientation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="16.0dip"
                        android:layout_marginTop="8.0dip"
                        android:text="@string/x8_ai_fly_follow_surround_orientation"
                        android:textColor="@color/white_90"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/x8_ai_line_rorate1"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_fc_item_x7"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="6.0dip"
                        android:padding="@dimen/x8s_main_fc_item_x1"
                        android:visibility="gone"
                        app:bg="#00000000"
                        app:default_index="0"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_fc_item_x1"
                        app:radiusC="@dimen/x8s_main_fc_item_x2"
                        app:src="@array/x8_ai_line_device_rorate1"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_fc_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/x8_ai_line_rorate2"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_fc_item_x7"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="6.0dip"
                        android:padding="@dimen/x8s_main_fc_item_x1"
                        android:visibility="gone"
                        app:bg="#00000000"
                        app:default_index="2"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_fc_item_x1"
                        app:radiusC="@dimen/x8s_main_fc_item_x2"
                        app:src="@array/x8_ai_line_device_rorate2"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_fc_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/tv_ai_tip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_rorate"
                    android:layout_marginLeft="16.0dip"
                    android:layout_marginTop="8.0dip"
                    android:text="@string/x8_ai_fly_follow_surround_tip"
                    android:textColor="@color/white_90"
                    android:textSize="10.0dip" />

                <RelativeLayout
                    android:id="@+id/rl_ai_excute_disconnect_exit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_tip"
                    android:layout_marginTop="14.0dip">

                    <TextView
                        android:id="@+id/tv_ai_excute_disconnect"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="16.0dip"
                        android:text="@string/x8_ai_fly_lines_excute_disconnect"
                        android:textColor="@color/white_90"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/tb_disconnect"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_fc_item_x7"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="6.0dip"
                        android:padding="@dimen/x8s_main_fc_item_x1"
                        app:bg="#00000000"
                        app:default_index="0"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_fc_item_x1"
                        app:radiusC="@dimen/x8s_main_fc_item_x2"
                        app:src="@array/x8_ai_line_excute_disconnect"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_fc_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_ai_excute_end"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_ai_excute_disconnect_exit"
                    android:layout_marginTop="14.0dip">

                    <TextView
                        android:id="@+id/tv_ai_excute_end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="16.0dip"
                        android:layout_marginTop="8.0dip"
                        android:text="@string/x8_ai_fly_lines_excute_end"
                        android:textColor="@color/white_90"
                        android:textSize="@dimen/textsize_39" />

                    <com.fimi.app.x8s.widget.X8TabHost
                        android:id="@+id/tb_ai_excute_end"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/x8s_main_fc_item_x7"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="6.0dip"
                        android:padding="@dimen/x8s_main_fc_item_x1"
                        app:bg="#00000000"
                        app:default_index="0"
                        app:lineColor="@color/white_25"
                        app:lineStroke="@dimen/x8s_main_fc_item_x1"
                        app:radiusC="@dimen/x8s_main_fc_item_x2"
                        app:src="@array/x8_ai_fly_lines_excute_end"
                        app:tab_select_color="@color/tabradio_attr_unselected_color"
                        app:tab_space="@dimen/x8s_main_fc_item_x1"
                        app:tab_unselect_color="@color/tabradio_attr_unselected_color"
                        app:text_select_color="@color/tabradio_attr_selected_text_color"
                        app:text_sizeC="@dimen/textsize_36"
                        app:text_unselect_color="@color/tabradio_attr_unselected_text_color" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/tv_ai_auto_record"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl_ai_excute_end"
                    android:layout_marginLeft="16.0dip"
                    android:layout_marginTop="8.0dip"
                    android:text="@string/x8_ai_fly_lines_auto_record"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39"
                    android:visibility="gone" />

                <com.fimi.widget.SwitchButton
                    android:id="@+id/sb_ai_auto_record"
                    android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x10"
                    android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x7"
                    android:layout_below="@+id/rl_ai_excute_end"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="8.0dip"
                    android:layout_marginRight="@dimen/x8s_ai_line_excute_confirm_layout_x8"
                    android:visibility="gone"
                    app:onColor="#ff09d9ff" />

                <TextView
                    android:id="@+id/tv_ai_curve"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_ai_auto_record"
                    android:layout_marginLeft="16.0dip"
                    android:layout_marginTop="8.0dip"
                    android:text="@string/x8_ai_fly_lines_curve"
                    android:textColor="@color/white_90"
                    android:textSize="@dimen/textsize_39"
                    android:visibility="gone" />

                <com.fimi.widget.SwitchButton
                    android:id="@+id/swb_ai_curve"
                    android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x10"
                    android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x7"
                    android:layout_below="@+id/tv_ai_auto_record"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="8.0dip"
                    android:layout_marginRight="@dimen/x8s_ai_line_excute_confirm_layout_x8"
                    android:visibility="gone"
                    app:onColor="#ff09d9ff" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btn_ai_follow_confirm_ok"
        android:layout_width="@dimen/x8s_ai_line_excute_confirm_layout_x12"
        android:layout_height="@dimen/x8s_ai_line_excute_confirm_layout_x9"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/x8_btn_white_selector"
        android:text="@string/x8_ai_fly_follow_go"
        android:textColor="@color/black_90"
        android:textSize="@dimen/textsize_39"
        app:layout_marginBottomPercent="2.777777%" />
</com.fimi.kernel.percent.PercentRelativeLayout>