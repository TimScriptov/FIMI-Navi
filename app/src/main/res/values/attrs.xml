        <attr name="x8_value_title" format="string" />
        <attr name="x8_value_suffix" format="string" />
        <attr name="x8_value_close_color" format="color" />
        <attr name="x8_value_open_color" format="color" />
        <attr name="x8_value_seekbar_max" format="float" />
