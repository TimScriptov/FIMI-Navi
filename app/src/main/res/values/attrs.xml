    <declare-styleable name="X8MainElectric">
        <attr name="lowElectric" format="color" />
        <attr name="middleElectric" format="color" />
        <attr name="hightElectric" format="color" />
    </declare-styleable>

    <declare-styleable name="X8MainPower">
        <attr name="image" format="reference" />
    </declare-styleable>

    <declare-styleable name="X8MainPitching">
        <attr name="bg_color" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="progress_color" format="color" />
        <attr name="progress_bar_width" format="dimension" />
        <attr name="progress_projection_color" format="color" />
        <attr name="progress_margin_left" format="dimension" />
